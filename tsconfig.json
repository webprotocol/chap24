{
  "compilerOptions": {
	  "target": "ES6",
      "module": "ES6",
      "outDir": "./src/main/resources/js",
      "incremental": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "jsx": "preserve",
      "moduleResolution": "Node"
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}